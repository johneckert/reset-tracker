{
  "name": "reset-tracker",
  "version": "0.0.1",
  "homepage": "https://johneckert.github.io/reset-tracker}",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "airtable": "^0.11.6",
    "axios": "^1.3.4",
    "eslint-plugin-import": "^2.27.5",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "turbo build-rewired",
    "test": "SKIP_VERSION_CHECK=1 react-app-rewired test --detectOpenHandles",
    "start": "turbo start-rewired",
    "start-no-lint": "DISABLE_ESLINT_PLUGIN=true turbo start",
    "start-rewired": "react-app-rewired start",
    "eject": "react-app-rewired eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --cache",
    "fix-format": "prettier ./src --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^28.1.0",
    "babel-plugin-transform-require-context": "^0.1.1",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^5.0.0",
    "lint": "^0.8.19",
    "lint-staged": "^10.5.1",
    "prettier": "2.1.2",
    "react-app-rewired": "^2.1.8",
    "ts-jest": "^27.1.3",
    "turbo": "^1.7.4"
  },
  "jest": {
    "transform": {
      "\\.[jt]s(x)?$": "ts-jest"
    }
  }
}
